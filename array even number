//1st array program
class Array_1
{ 
// main method
public static void main (string[]args)
{
Scanner sc=new Scanner(System.in)
{
 // variable declaration of int datatype,element value is 10
int int_arr[]=new int[10];
System.out.println("entre the element of int datatype")
 // to repatation of for loop
for (int=0,i<int_arr.lenght;i++)
{
int_arr[i]sc.nextint();
} // end for loop
} // end main method
} // end class block
